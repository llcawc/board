{
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "rootDir": "./",
    "outDir": "./",
    // "skipLibCheck": true,
    // "resolveJsonModule": true,
    // "sourceMap": true,
    // "removeComments": true,
    "declaration": true,
    "newLine": "lf",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true
  },
  "include": ["./"],
  "exclude": []
}
