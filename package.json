    "animate.css": "^4.1.1",
    "bootstrap-icons": "^1.13.1"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@tailwindcss/oxide",
      "cwebp-bin",
      "esbuild",
      "gifsicle",
      "mozjpeg",
