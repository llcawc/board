    "@types/jsdom": "^27.0.0",
    "@types/node": "^24.9.1",
    "@vituum/vite-plugin-pug": "^1.1.0",
    "del": "^8.0.1",
    "eslint": "^9.38.0",
    "globals": "^16.4.0",
    "gulp": "^5.0.1",
    "gulp-changed": "^5.0.4",
    "gulp-replace": "^1.1.4",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jsdom": "^27.0.1",
    "prettier": "^3.6.2",
    "prettier-plugin-embed": "^0.5.0",
    "prettier-plugin-organize-imports": "^4.3.0",
